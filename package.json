    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "parcel": "^1.12.4",
    "preact-cli": "^3.0.0-rc.6",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "^0.4.5"
  },
  "dependencies": {
    "module-alias": "^2.2.2",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hexagon": "^1.1.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "vanta": "^0.5.21"
